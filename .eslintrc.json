{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y"],
  "rules": {
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1
  }
}
